{
  "name": "git-version-differ",
  "version": "1.0.0",
  "description": "A CLI tool and Vercel function to get commit information between two Git tags or commit hashes from a GitHub repository",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "cli": "node index.js",
    "test": "node test-all.js",
    "test-core": "node test-core.js",
    "local-server": "node local-server.js",
    "api-dev": "node local-server.js",
    "vercel-dev": "vercel dev",
    "vercel-deploy": "vercel --prod"
  },
  "keywords": [
    "git",
    "github",
    "cli",
    "commits",
    "version",
    "diff",
    "vercel",
    "serverless",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@octokit/graphql": "^9.0.1",
    "@octokit/rest": "^20.0.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.6.4",
  "devDependencies": {
    "@vercel/node": "^5.3.0"
  }
}
